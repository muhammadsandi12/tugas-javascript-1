const printSegitiga = (inputAngka) =>{
let output=''
if(typeof(inputAngka) === 'number'){
    for(let i = inputAngka; i>0; i--){
        for(let j = 1; j <= i; j++ ){
            output += j
        }
        output += '\n'
    }
}else{
    output = 'Data harus number'
}
    return output
} 


console.log(printSegitiga(5))
